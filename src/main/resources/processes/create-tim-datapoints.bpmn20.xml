  <collaboration id="Collaboration">
    <participant id="pool1" name="Creating points for monitoring by TIM" processRef="createTimPoints"></participant>
  </collaboration>
  <process id="createTimPoints" name="createTimPoints" isExecutable="true">
    <laneSet id="laneSet_createTimPoints">
      <lane id="lane1" name="REQUESTOR">
        <flowNodeRef>correctErrors</flowNodeRef>
        <flowNodeRef>submitRequest</flowNodeRef>
      </lane>
      <lane id="lane2" name="MODESTI">
        <flowNodeRef>validateRequest</flowNodeRef>
        <flowNodeRef>configurePoints</flowNodeRef>
        <flowNodeRef>alarmCheck</flowNodeRef>
        <flowNodeRef>errorCheck</flowNodeRef>
      </lane>
      <lane id="lane3" name="TI OPERATOR">
        <flowNodeRef>approveAlarms</flowNodeRef>
        <flowNodeRef>testPoints</flowNodeRef>
      </lane>
    </laneSet>
    <endEvent id="end" name="End"></endEvent>
    <startEvent id="start" name="Start"></startEvent>
    <userTask id="approveAlarms" name="Approve alarms" activiti:candidateGroups="modesti-developers"></userTask>
    <sequenceFlow id="flow4" sourceRef="approveAlarms" targetRef="configurePoints">
      <extensionElements>
        <activiti:executionListener event="take" expression="${requestStatusManager.setRequestStatus(requestId, &quot;FOR_CONFIGURATION&quot;)}"></activiti:executionListener>
      </extensionElements>
    </sequenceFlow>
    <serviceTask id="configurePoints" name="Configure points" activiti:class="cern.modesti.workflow.task.ConfigurePointsTask"></serviceTask>
    <sequenceFlow id="flow5" sourceRef="configurePoints" targetRef="testPoints">
      <extensionElements>
        <activiti:executionListener event="take" expression="${requestStatusManager.setRequestStatus(requestId, &quot;FOR_TESTING&quot;)}"></activiti:executionListener>
      </extensionElements>
    </sequenceFlow>
    <userTask id="testPoints" name="Test points" activiti:candidateGroups="nonexistent"></userTask>
    <sequenceFlow id="flow6" sourceRef="testPoints" targetRef="end">
      <extensionElements>
        <activiti:executionListener event="take" expression="${requestStatusManager.setRequestStatus(requestId, &quot;CLOSED&quot;)}"></activiti:executionListener>
      </extensionElements>
    </sequenceFlow>
    <serviceTask id="validateRequest" name="Validate Request" activiti:expression="${requestValidationTask.validateRequest(requestId, execution)}"></serviceTask>
    <userTask id="correctErrors" name="Correct Errors" activiti:assignee=" "></userTask>
    <sequenceFlow id="flow11" name="validation failed" sourceRef="errorCheck" targetRef="correctErrors">
      <extensionElements>
        <activiti:executionListener event="take" expression="${requestStatusManager.setRequestStatus(requestId, &quot;FOR_CORRECTION&quot;)}"></activiti:executionListener>
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${containsErrors}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="alarmCheck" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow2" sourceRef="alarmCheck" targetRef="approveAlarms">
      <extensionElements>
        <activiti:executionListener event="take" expression="${requestStatusManager.setRequestStatus(requestId, &quot;FOR_APPROVAL&quot;)}"></activiti:executionListener>
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${containsAlarms}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow3" sourceRef="alarmCheck" targetRef="configurePoints">
      <extensionElements>
        <activiti:executionListener event="take" expression="${requestStatusManager.setRequestStatus(requestId, &quot;FOR_CONFIGURATION&quot;)}"></activiti:executionListener>
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!containsAlarms}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow12" name="validation passed" sourceRef="errorCheck" targetRef="alarmCheck">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!containsErrors}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="errorCheck" name="Parallel Gateway"></exclusiveGateway>
    <userTask id="submitRequest" name="Submit Request"></userTask>
    <sequenceFlow id="flow15" sourceRef="submitRequest" targetRef="validateRequest"></sequenceFlow>
    <sequenceFlow id="flow16" name="request created" sourceRef="start" targetRef="submitRequest"></sequenceFlow>
    <sequenceFlow id="flow17" sourceRef="validateRequest" targetRef="errorCheck">
      <extensionElements>
        <activiti:executionListener event="take" expression="${requestStatusManager.setRequestStatus(requestId, &quot;IN_PROGRESS&quot;)}"></activiti:executionListener>
      </extensionElements>
    </sequenceFlow>
    <sequenceFlow id="flow18" sourceRef="correctErrors" targetRef="validateRequest"></sequenceFlow>
    <textAnnotation id="textannotation1">
      <text>TODO: handle alarm approval rejection
TODO: handle configuration failure
TODO: handle addressing and cabling</text>
    </textAnnotation>
