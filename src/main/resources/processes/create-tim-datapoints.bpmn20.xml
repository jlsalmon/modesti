    <sequenceFlow id="flow6" sourceRef="testPoints" targetRef="closeRequest"></sequenceFlow>
    <serviceTask id="configurePoints" name="Configure points" activiti:class="cern.modesti.workflow.ConfigurePointsTask"></serviceTask>
    <serviceTask id="closeRequest" name="Close request" activiti:class="cern.modesti.workflow.CloseRequestTask"></serviceTask>
    <sequenceFlow id="flow7" sourceRef="closeRequest" targetRef="end"></sequenceFlow>
    <endEvent id="end" name="End"></endEvent>
