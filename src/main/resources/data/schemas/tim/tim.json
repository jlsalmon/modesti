{
  "name": "tim",
  "extends": "core",
  "categories": [
    {
      "name": "General",
      "constraints": [
        {
          "type": "or",
          "members": ["gmaoCode", "otherEquipCode"]
        }
      ],
      "fields": [
        {
          "id": "gmaoCode",
          "type": "autocomplete",
          "minLength": 4,
          "url": "gmaoCodes/search/find",
          "name_en": "GMAO Code",
          "name_fr": "Code GMAO",
          "help_en": "The equipment code known by the GMAO system",
          "help_fr": "Le code de l'equipement connu par le système GMAO"
        },
        {
          "id": "otherEquipCode",
          "type": "text",
          "name_en": "Other Code",
          "name_fr": "Autre Code",
          "help_en": "A code identifying the equipment concerned if no GMAO code exists, or sub-equipment code that complements the GMAO code",
          "help_fr": "Un code identifiant l'equipement concerne si aucun code GMAO existe, ou le code du sous-équipement qui complète le code GMAO"
        },
        {
          "id": "pointCompInfo",
          "type": "text",
          "name_en": "Complementary Info",
          "name_fr": "Complementary Info",
          "help_en": "",
          "help_fr": ""
        },
        {
          "id": "monitoringEquipment",
          "type": "autocomplete",
          "url": "monitoringEquipments/search/find",
          "required": true,
          "name_en": "Equipment Name",
          "name_fr": "Equipment Name",
          "help_en": "",
          "help_fr": ""
        },
        {
          "id": "cabling",
          "type": "checkbox",
          "name_en": "Cabling",
          "name_fr": "Cablage",
          "help_en": "",
          "help_fr": ""
        },
        {
          "id": "detail",
          "type": "text",
          "name_en": "Detail",
          "name_fr": "Detail",
          "help_en": "Supplementary details of the localisation (e.g. Racks)",
          "help_fr": "Details supplementaire de la localisation (e.g. Racks)"
        }
      ]
    },
    {
      "name": "Alarms",
      "fields": [
        {
          "id": "alarmCategory",
          "type": "autocomplete",
          "url": "alarmCategories/search/find",
          "name_en": "Category",
          "name_fr": "Categorie",
          "help_en": "The alarm class in which the alarm will be active",
          "help_fr": "La categorie d'alarme dans laquelle l'alarme sera diffusee"
        },
        {
          "id": "autocallNumber",
          "type": "numeric",
          "name_en": "Autocall Number",
          "name_fr": "Autocall Number",
          "help_en": "Enter '1' to indicate that an automatic call will be made by the alarm, or the Mobicall number (even number between 7000 and 7998) if known",
          "help_fr": "Entrez '1' pour indiquer qu'un appel automatique sera effectua par l'alarme, ou le numero Mobicall (nombre pair entre 7000 et 7998) si connu"
        },
        {
          "id": "multiplicityValue",
          "type": "numeric",
          "name_en": "Multiplicity Value",
          "name_fr": "Valeur Multiplicité",
          "help_en": "",
          "help_fr": ""
        },
        {
          "id": "parentAlarm",
          "type": "numeric",
          "name_en": "Parent Alarm",
          "name_fr": "Alarme Parent",
          "help_en": "",
          "help_fr": ""
        },
        {
          "id": "electricityFaultFamily",
          "type": "text",
          "name_en": "Electricity Fault Family",
          "name_fr": "Famille Défaut électricité",
          "help_en": "",
          "help_fr": ""
        }
      ]
    }
  ]
}